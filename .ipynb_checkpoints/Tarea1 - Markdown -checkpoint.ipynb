    "    for j in range(2,i):\n",
    "        if i == j:\n",
    "           break\n",
    "        elif i%j == 0:\n",
    "           primos = False\n",
    "        else:\n",
    "           continue\n",
    "    if primos == True:\n",
    "        print(' ',i, end='')\n",
    "        cont += 1\n",
    "print('\\nHay %u números primos.' % cont )"
   ]
